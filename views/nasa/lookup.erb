<h1>Lookup Asteroid by ID</h1>
<form id="user_search_id">
  <div class="form-group">
    <input type="text" class="form-control" placeholder="Search" style="width:200px;">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
<table class="table table-sm">
  <!-- <th>Asteroid Feed</th> -->
  <tr>
    <td> Id </td><td> <%= @keys['neo_reference_id'] %> </td>
  </tr>
  <tr>
    <td> Name </td><td> <%= @keys['name'] %> </td>
  </tr>
  <tr>
    <td> Nasa URL </td><td> <%= @keys['nasa_jpl_url'] %> </td>
  </tr>
  <tr>
    <td> Magnitude </td><td> <%= @keys['absolute_magnitude_h'] %> </td>
  </tr>
  <tr>
    <td> Estimated min diameter</td><td> <%= @keys['estimated_diameter']['kilometers']['estimated_diameter_min'] %> km </td>
  </tr>
  <tr>
    <td> Estimated max diameter </td><td> <%= @keys['estimated_diameter']['kilometers']['estimated_diameter_max'] %> km</td>
  </tr>
  <tr>
    <td> Hazardous? </td><td> <%= @keys['is_potentially_hazardous_asteroid'] %> </td>
  </tr>
  <tr>
    <td> Close Approach date </td><td> <%= @feed['close_approach_date'] %> </td>
  </tr>
  <tr>
    <td> Epoch close Approach date </td><td> <%= @feed['epoch_date_close_approach'] %> </td>
  </tr>
  <tr>
    <td> Velocity </td><td> <%= @feed['relative_velocity']['miles_per_hour'] %> mph</td>
  </tr>
  <tr>
    <td> Miss distance </td><td> <%= @feed['miss_distance']['miles'] %> miles</td>
  </tr>
  <tr>
    <td> Orbiting Body </td><td> <%= @feed['orbiting_body'] %></td>
  </tr>
</table>
