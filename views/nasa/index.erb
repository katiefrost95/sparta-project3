<h1>Today's Asteroid Feed</h1>
<table class="table table-sm">
  <!-- <th>Asteroid Feed</th> -->
  <tr>
    <td> Id </td><td> <%= @feed['neo_reference_id'] %> </td>
  </tr>
  <tr>
    <td> Name </td><td id="asteroid_feed_name"> <%= @feed['name'] %> </td>
  </tr>
  <tr>
    <td> Nasa URL </td><td> <%= @feed['nasa_jpl_url'] %> </td>
  </tr>
  <tr>
    <td> Magnitude </td><td> <%= @feed['absolute_magnitude_h'] %> </td>
  </tr>
  <tr>
    <td> Estimated min diameter</td><td> <%= @feed['estimated_diameter']['kilometers']['estimated_diameter_min'] %> km </td>
  </tr>
  <tr>
    <td> Estimated max diameter </td><td> <%= @feed['estimated_diameter']['kilometers']['estimated_diameter_max'] %> km</td>
  </tr>
  <tr>
    <td> Hazardous? </td><td> <%= @feed['is_potentially_hazardous_asteroid'] %> </td>
  </tr>
  <tr>
    <td> Close Approach date </td><td> <%= @feed['close_approach_data'][0]['close_approach_date'] %> </td>
  </tr>
  <tr>
    <td> Velocity </td><td> <%= @feed['close_approach_data'][0]['relative_velocity']['miles_per_hour'] %> mph</td>
  </tr>
  <tr>
    <td> Miss distance </td><td> <%= @feed['close_approach_data'][0]['miss_distance']['miles'] %> miles</td>
  </tr>
  <tr>
    <td> Orbiting Body </td><td> <%= @feed['close_approach_data'][0]['orbiting_body'] %></td>
  </tr>
</table>

<a href="/"><button id="feed_refresh_button" type="button" class="btn btn-default">New Asteroid</button></a>
