<h1>Browse All Asteroids</h1>
<table class="table table-sm">
  <!-- <th>Asteroid Feed</th> -->
  <tr>
    <td> Id </td><td> <%= @feed['neo_reference_id'] %> </td>
  </tr>
  <tr>
    <td> Name </td><td> <%= @feed['name'] %> </td>
  </tr>
  <tr>
    <td> Nasa URL </td><td> <%= @feed['nasa_jpl_url'] %> </td>
  </tr>
  <tr>
    <td> Magnitude </td><td> <%= @feed['absolute_magnitude_h'] %> </td>
  </tr>
  <tr>
    <td> Estimated min diameter</td><td> <%= @feed['estimated_diameter']['kilometers']['estimated_diameter_min'] %> km </td>
  </tr>
  <tr>
    <td> Estimated max diameter </td><td> <%= @feed['estimated_diameter']['kilometers']['estimated_diameter_max'] %> km</td>
  </tr>
  <tr>
    <td> Hazardous? </td><td> <%= @feed['is_potentially_hazardous_asteroid'] %> </td>
  </tr>
  <% if @feed['close_approach_data'].empty? %>
    <p></p>
  <% else %>
    <tr>
      <td> Close Approach date </td><td> <%= @feed['close_approach_data'][0]['close_approach_date'] %> </td>
    </tr>
    <tr>
      <td> Velocity </td><td> <%= @feed['close_approach_data'][0]['relative_velocity']['miles_per_hour'] %> mph</td>
    </tr>
    <tr>
      <td> Miss distance </td><td> <%= @feed['close_approach_data'][0]['miss_distance']['miles'] %> miles</td>
    </tr>
    <tr>
      <td> Orbiting Body </td><td> <%= @feed['close_approach_data'][0]['orbiting_body'] %></td>
    </tr>
  <% end %>
  <% if @feed['close_approach_data'][1] %>
    <tr>
      <td> Close Approach date </td><td> <%= @feed['close_approach_data'][1]['close_approach_date'] %> </td>
    </tr>
    <tr>
      <td> Velocity </td><td> <%= @feed['close_approach_data'][1]['relative_velocity']['miles_per_hour'] %> mph</td>
    </tr>
    <tr>
      <td> Miss distance </td><td> <%= @feed['close_approach_data'][1]['miss_distance']['miles'] %> miles</td>
    </tr>
    <tr>
      <td> Orbiting Body </td><td> <%= @feed['close_approach_data'][1]['orbiting_body'] %></td>
    </tr>
  <% end %>
  <tr>
    <td> Orbit ID </td><td> <%= @feed['orbital_data']['orbit_id'] %></td>
  </tr>
  <tr>
    <td> Orbit Determination Date</td><td> <%= @feed['orbital_data']['orbit_determination_date'] %></td>
  </tr>
  <tr>
    <td> Equinox</td><td> <%= @feed['orbital_data']['equinox'] %></td>
  </tr>



</table>
